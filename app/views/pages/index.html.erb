<div class="page-header">
	<h1>Welcome! 
		<% if logged_in? %>
			<span><%= current_user.full_name %></span>
		<% end %>
	</h1>
</div>

<div id="flash">
  <% flash.each do |key, value| %>
    <div class="flash <%= key %>"><%= value %></div>
<% end %>

<%= button_to ("Sign out"), sign_out_path, method: :delete, class: 'btn btn-primary' %>








